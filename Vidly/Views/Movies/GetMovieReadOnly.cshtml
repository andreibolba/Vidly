@model Vidly.Models.Movie
@{
    ViewBag.Title = "GetMovieReadOnly";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .member {
        font-size: 20px;
        padding-bottom: 10px;
    }
</style>
<h1>Normal User</h1>
<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre</li>
    <li>Relase date: @Model.Released.ToString("dddd, MMMM dd, yyyy")</li>
    <li>Data Added: @Model.Added.ToString("dddd, MMMM dd, yyyy")</li>
    <li>Number in Stock: @Model.Stock</li>
</ul>